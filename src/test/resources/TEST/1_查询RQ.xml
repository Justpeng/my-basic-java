        <ns6:OTA_AirLowFareSearchRQ xmlns:ns1="http://www.ebxml.org/namespaces/messageHeader" xmlns:ns2="http://schemas.xmlsoap.org/soap/envelope/" xmlns:ns3="http://www.w3.org/1999/xlink" xmlns:ns4="http://schemas.xmlsoap.org/ws/2002/12/secext" xmlns:ns6="http://www.opentravel.org/OTA/2003/05" xmlns:ns7="http://www.w3.org/2000/09/xmldsig#" AvailableFlightsOnly="true" ResponseType="OTA" ResponseVersion="3.1.0" Target="Production" Version="3.1.0">
            <ns6:POS>
                <ns6:Source PseudoCityCode="67X8"> <!--pcc-->
                    <ns6:RequestorID Type="1" ID="1">
                        <ns6:CompanyName Code="TN"/>
                    </ns6:RequestorID>
                </ns6:Source>
            </ns6:POS>
            <ns6:OriginDestinationInformation>
                <!--去程信息，出发日期 11:00:00 可获取更多优势资源-->
                <ns6:DepartureDateTime>2017-08-25T11:00:00</ns6:DepartureDateTime>
                <ns6:DepartureWindow>00002359</ns6:DepartureWindow>
                <ns6:OriginLocation LocationCode="BOS"/>
                <ns6:DestinationLocation LocationCode="SFO"/>
                <ns6:TPA_Extensions><!--仓等-->
                    <ns6:CabinPref Cabin="Y" PreferLevel="Preferred"/>
                </ns6:TPA_Extensions>
            </ns6:OriginDestinationInformation>
            <ns6:OriginDestinationInformation>
                <!--返程信息-->
                <ns6:DepartureDateTime>2017-08-30T11:00:00</ns6:DepartureDateTime>
                <ns6:DepartureWindow>00002359</ns6:DepartureWindow>
                <ns6:OriginLocation LocationCode="SFO"/>
                <ns6:DestinationLocation LocationCode="BOS"/>
                <ns6:TPA_Extensions>
                    <ns6:CabinPref Cabin="Y" PreferLevel="Preferred"/>
                </ns6:TPA_Extensions>
            </ns6:OriginDestinationInformation>
            <ns6:TravelPreferences ValidInterlineTicket="true" MaxStopsQuantity="2">
                <ns6:FlightTypePref MaxConnections="1"/>
                <ns6:CabinPref Cabin="Y" PreferLevel="Preferred"/>
                <ns6:TPA_Extensions>
                    <ns6:OnlineIndicator Ind="true"/>
                    <ns6:TripType Value="Return"/>
                    <!--XOFares参数的作用是指定sabre是否强制返回对应旅客类型的价格信息。如果为true，则强制返回，
                     如果没有对应旅客类型的价格信息，则此航班组合不返回；
                    如果为false，同时没有对应旅客类型的价格信息，则sabre会返回成人价格给儿童-->
                    <!--1 新加坡市场要求XOFares 设定为true，其他市场可以为false-->
                    <!--2 BFM-RQ的XOFares参数值为true时需修改EAB接口旅客信息参数，BargainFinder的PassengerType增加Force="true"-->
                    <ns6:XOFares Value="false"/>

                    <ns6:JumpCabinLogic Disabled="true"/> <!--防止跳舱-->
                    <ns6:KeepSameCabin Enabled="true"/> <!--JumpCabinLogic不能完全避免包含舱等不一致的航班组合返回，需增加KeepSameCabin参数规避此问题-->
                    <ns6:ExcludeCallDirectCarriers Enabled="true"/>
                    <ns6:DiversityParameters AdditionalNonStopsNumber="9"><!--在已定查询数量上，多返回9条数据-->
                        <ns6:Weightings PriceWeight="7" TravelTimeWeight="3"/><!--权重-->
                    </ns6:DiversityParameters>
                </ns6:TPA_Extensions>
            </ns6:TravelPreferences>
            <ns6:TravelerInfoSummary>
                <ns6:AirTravelerAvail>
                    <ns6:PassengerTypeQuantity Code="ADT" Quantity="1"/>
                    <ns6:PassengerTypeQuantity Code="CNN" Quantity="1"/>
                </ns6:AirTravelerAvail>
                <ns6:PriceRequestInformation CurrencyCode="CNY" ProcessThruFaresOnly="false">
                    <ns6:TPA_Extensions>
