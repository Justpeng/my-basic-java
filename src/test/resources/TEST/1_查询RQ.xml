                            <ns6:DirectFlights Priority="2"/>
                            <ns6:Time Priority="3"/>
                            <ns6:Vendor Priority="4"/>
                        </ns6:Priority>
                    </ns6:TPA_Extensions>
                </ns6:PriceRequestInformation>
            </ns6:TravelerInfoSummary>
            <ns6:TPA_Extensions>
